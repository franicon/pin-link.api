  "devDependencies": {
    "jest": "28.1.3",
    "eslint": "^8.0.1",
    "ts-node": "^10.0.0",
    "ts-jest": "28.0.8",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.3",
    "supertest": "^6.1.3",
    "typescript": "^4.7.4",
    "@nestjs/cli": "^9.0.0",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/express": "^4.17.13",
